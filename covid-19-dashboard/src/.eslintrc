{
  "rules": {
    "prettier/prettier": ["error"],
    "import/prefer-default-export": "off",
    "no-underscore-dangle": "off",
    "no-return-assign": "off",
    "no-console": "off",
    "no-new": "off",
    "no-bitwise": "off",
    "no-param-reassign": "off"
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"]
}
